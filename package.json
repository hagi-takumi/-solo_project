  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "concurrently": "^8.1.0",
    "doctoc": "^2.2.1",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "swiper": "^9.3.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm install && npm run migrate-back && npm run migrate-latest && npm run seed-data && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node ./src/Back/server.js",
    "dev": "nodemon ./src/Back/server.js",
    "migrate-latest": "npx knex migrate:latest --knexfile src/Back/data/knexfile.js",
    "migrate-make": "npx knex migrate:make --knexfile src/Back/data/knexfile.js",
    "migrate-back": "npx knex migrate:rollback --knexfile src/Back/data/knexfile.js",
    "migrate-down": "npx knex migrate:down --knexfile src/Back/data/knexfile.js",
    "seed-make": "npx knex seed:make --knexfile src/Back/data/knexfile.js",
    "seed-data": "npx knex seed:run --knexfile src/Back/data/knexfile.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
